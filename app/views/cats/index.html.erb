<table>

  <th>Name</th>
  <th>Color</th>
  <th>Age</th>
  <th>Sex</th>
  <th>Description</th>
  
  <% @cats.each do |cat| %>
  <tr>
    <td><%= cat.name %></td>
    <td><%= cat.color %></td>
    <td><%= cat.age %></td>
    <td><%= cat.sex %></td>
    <td><%= cat.description %></td>
  </tr>
  <% end %>
</table>
